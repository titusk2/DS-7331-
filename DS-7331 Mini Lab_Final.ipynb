    "import pandas as pd\n",
    "from sklearn.svm import SVC\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "# Recreate X and y \n",
    "target = \"default payment next month\"\n",
    "y = df[target].astype(int)\n",
    "X = df.drop(columns=[target])\n",
    "\n",
    "# Train/test split \n",
    "from sklearn.model_selection import train_test_split\n",
    "X_train, X_test, y_train, y_test = train_test_split(\n",
    "    X, y, test_size=0.2, random_state=0, stratify=y\n",
    ")\n",
    "\n",
    "# Scale features\n",
    "scaler = StandardScaler()\n",
    "X_train_scaled = scaler.fit_transform(X_train)\n",
    "\n",
    "# Re-train SVM (linear)\n",
    "svm_model = SVC(kernel=\"linear\", C=1.0, probability=True, random_state=0)\n",
    "svm_model.fit(X_train_scaled, y_train)\n",
    "\n",
    "# ---- Inspect support vectors ----\n",
    "print(\"Number of support vectors per class:\", svm_model.n_support_)\n",
    "\n",
    "sv_idx = svm_model.support_\n",
    "support_vectors_df = X_train.iloc[sv_idx]\n",
    "\n",
    "support_vectors_df.head(10)\n"
   ]
